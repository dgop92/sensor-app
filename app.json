{
  "expo": {
    "name": "sensor-app",
    "slug": "sensor-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D4052"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-adap-icon.png",
        "backgroundColor": "#2D4052"
      },
      "package": "com.inevaup.app.sensorapp",
      "versionCode": 1,
      "permissions": ["WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/app-favicon"
    },
    "description": "an application for visualizaci√≥n and data record of vectorial physics sensors"
  }
}
